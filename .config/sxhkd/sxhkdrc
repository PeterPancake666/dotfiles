# Printscreen

Print
  flameshot gui

# Powermenu

super + shift + q
  /home/petrup/.local/bin/powermenu

# Rofi

super + alt + e  
  rofi -show emoji

super + alt + c
  rofi -show calc

# Audio and media

XF86AudioMute
  /home/petrup/.local/bin/volume mute

XF86AudioLowerVolume
  /home/petrup/.local/bin/volume down

XF86AudioRaiseVolume
  /home/petrup/.local/bin/volume up

XF86AudioPlay
  /home/petrup/.local/bin/mediacontrol toggle

XF86AudioNext
  /home/petrup/.local/bin/mediacontrol next

XF86AudioPrev
  /home/petrup/.local/bin/mediacontrol previous

XF86AudioStop
  /home/petrup/.local/bin/mediacontrol stop


# Backlight

XF86MonBrightnessUp
  /home/petrup/.local/bin/backlight up

XF86MonBrightnessDown
  /home/petrup/.local/bin/backlight down
